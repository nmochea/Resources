metadata:
  language: v1-beta
  name: "AWS ELB"
  description: "Looks for AWS ELB regex in page source."
  author: "@nmochea"
  tags: "nmochea", "token", "exposure"

given response then
  if {latest.response} matches "[0-9a-z.-_]+.elb.amazonaws.com" then
     report issue:
       severity: info
       confidence: tentative
       detail: "AWS ELB regex found in page."
       remediation: "Review and remove unnecessary exposure of keys and/or sensitive data from page source."
  end if
