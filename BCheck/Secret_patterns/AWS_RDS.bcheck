metadata:
  language: v1-beta
  name: "AWS RDS"
  description: "Looks for AWS RDS regex in page source."
  author: "@nmochea"
  tags: "nmochea", "token", "exposure"

given response then
  if {latest.response} matches "[0-9a-z.-_]+.rds.amazonaws.com" then
     report issue:
       severity: info
       confidence: tentative
       detail: "AWS RDS regex found in page."
       remediation: "Review and remove unnecessary exposure of keys and/or sensitive data from page source."
  end if
