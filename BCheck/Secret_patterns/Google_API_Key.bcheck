metadata:
  language: v1-beta
  name: "Google API Key"
  description: "Looks for Google API Key regex in page source."
  author: "@nmochea"
  tags: "nmochea", "token", "exposure"

given response then
  if {latest.response} matches "AIza[0-9a-z-_]{35}" then
     report issue:
       severity: info
       confidence: tentative
       detail: "Google API Key regex found in page."
       remediation: "Review and remove unnecessary exposure of keys and/or sensitive data from page source."
  end if
